{
				"name": "polychrome",
				"displayName": "Polychrome",
				"description": "Official VSCode extension for the Polychrome editor.",
				"version": "0.0.1",
				"engines": {
								"vscode": "^1.66.0"
				},
				"categories": [
								"Other"
				],
				"activationEvents": [
								"*"
				],
				"main": "./out/extension.js",
				"contributes": {
								"commands": [
												{
																"title": "Enable CodeLens",
																"command": "polychrome.enableCodeLens",
																"category": "Polychrome"
												},
												{
																"title": "Disable Codelens",
																"command": "polychrome.disableCodeLens",
																"category": "Polychrome"
												}
								],
								"configuration": {
												"properties": {
																"polychrome.enableCodeLens": {
																				"type": "boolean",
																				"default": true
																}
												}
								},
								"views": {
									"explorer": [
										{
											"id": "polychrome.assets",
											"name": "Polychrome Assets"
										}
									]
								}
				},
				"scripts": {
								"vscode:prepublish": "npm run compile",
								"compile": "tsc -p ./",
								"watch": "tsc -watch -p ./",
								"pretest": "npm run compile && npm run lint",
								"lint": "eslint src --ext ts",
								"test": "node ./out/test/runTest.js"
				},
				"devDependencies": {
								"@types/glob": "^7.2.0",
								"@types/mocha": "^9.1.0",
								"@types/node": "14.x",
								"@types/vscode": "^1.66.0",
								"@typescript-eslint/eslint-plugin": "^5.16.0",
								"@typescript-eslint/parser": "^5.16.0",
								"@vscode/test-electron": "^2.1.3",
								"esbuild": "^0.25.0",
								"eslint": "^8.11.0",
								"glob": "^7.2.0",
								"mocha": "^11.7.4",
								"typescript": "^4.5.5"
				},
				"dependencies": {
								"vsce": "^2.7.0",
								"yaml": "^1.10.2"
				}
}
